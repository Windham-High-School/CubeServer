{
	"name": "CuberServer",
	"dockerComposeFile": "docker-compose.yml",
	"service": "combined",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

	"forwardPorts": [
		8080,
		8081
	],
  
	"customizations": {
		"vscode": {
			"extensions": [
				"lfs.vscode-emacs-friendly",
				"ms-python.isort",
				"ms-python.pylint"
			]
		}
	}
}