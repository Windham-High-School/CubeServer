{
	"name": "CuberServer",
	"dockerComposeFile": "docker-compose.yml",
	"service": "combined",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"forwardPorts": [
		8080,
		8081
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.isort",
				"ms-python.black-formatter",
				"ms-python.mypy-type-checker",
				"charliermarsh.ruff",
				"samuelcolvin.jinjahtml"
			]
		}
	}
}